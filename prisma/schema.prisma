generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model notifyvc {
  id          BigInt @id @unique(map: "id") @default(autoincrement()) @db.UnsignedBigInt
  guild_id    BigInt @db.BigInt @unique
  set_channel BigInt @db.BigInt
}
